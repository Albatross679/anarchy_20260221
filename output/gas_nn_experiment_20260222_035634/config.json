{
  "name": "gas_nn_experiment",
  "model_type": "nn_experiment",
  "seed": 42,
  "output_dir": "output",
  "tensorboard": {
    "enabled": true,
    "log_system_metrics": true,
    "log_cpu": true,
    "log_gpu_utilization": true,
    "log_vram": true,
    "log_gpu_temp": true,
    "log_gpu_power": true,
    "log_hparams_text": true,
    "log_histograms": true,
    "histogram_every_n_epochs": 5
  },
  "output": {
    "base_dir": "output",
    "save_config": true,
    "timestamp_format": "%Y%m%d_%H%M%S",
    "subdirs": {
      "plots": "plots"
    }
  }
}